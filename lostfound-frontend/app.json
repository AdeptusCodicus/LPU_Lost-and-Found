{
  "expo": {
    "name": "LPU Lost and Found",
    "slug": "lpu-laf",
    "version": "1.0.0-alpha.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "works.valdoria-software.lpulaf"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#800000"
      },
      "edgeToEdgeEnabled": false,
      "package": "works.valdoria_software.lpulaf"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "eee85606-ed32-411c-ba9e-43d16a0e70c3"
      }
    }
  }
}
